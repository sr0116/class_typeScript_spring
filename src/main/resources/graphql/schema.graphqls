type Employee {
    id: ID!
    name: String!
    age: Int!
    job: String!
    language: String!
    pay: Int!
}

input EmployeeInput {
    name: String!
    age: Int!
    job: String!
    language: String!
    pay: Int!


}

type Query {
    employees: [Employee!]!
    employee(id: ID!): Employee
}

type Mutation {
    createEmployee(input: EmployeeInput!): Employee!
    updateEmployee(id: ID!, input: EmployeeInput!): Employee!
    deleteEmployee(id: ID!): Boolean!
}
